{
  "version": 2,
  "builds": [
    {
      "src": "KVB-frontend/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "https://your-backend-deployment-url.com/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/KVB-frontend/dist/$1"
    }
  ],
  "outputDirectory": "KVB-frontend/dist"
}
